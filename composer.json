{
	"name": "kaanyakar/laravel-pos",
	"type": "package",
	"description": "Laravel 5 Pos Package",
	"keywords": ["laravel5 pos", "laravel5 sanal pos"],
	"homepage": "https://github.com/kaanyakar/laravel-pos",
	"license": "MIT",
	"authors": [
		{
			"name": "Siparislio",
			"email": "me@Kaanyakar.com",
			"homepage": "https://github.com/Kaanyakar",
			"role": "Developer"
		}
	],
	"require": {
		"php": "^8.0.2",
		"illuminate/config": "*",
		"mews/pos": "*"
	},
	"require-dev": {
		"orchestra/testbench": "*",
		"phpunit/phpunit": "*"
	},
	"autoload": {
		"psr-4": {
			"Kaanyakar\\LaravelPos\\": "src/"
		},
		"files": [
			"src/helpers.php"
		]
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		],
		"psr-4": {
			"Mews\\Test\\": "tests/"
		}
	},
	"minimum-stability": "stable"
}
